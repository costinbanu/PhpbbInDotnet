@model PhpbbInDotnet.Forum.Pages.CustomPartials._HeaderLinksPartialModel

    <span onclick="toggleHeaderLinks('headerLinks', 'metrouusorLinks');" id="headerLinksButton" class="MyButton MyMiniButton" style="line-height:1.1em">
        &#X1F6E0;&nbsp;Meniu
    </span>
<ul class="Shortcuts" style="display:none" id="headerLinks">
    <li>
        <span> &nbsp;</span>
        <a asp-page="ViewForum" asp-page-handler="ownPosts" class="nowrap">Subiecte proprii</a>
    </li>
    <li>
        <span> &nbsp;</span>
        <a asp-page="ViewForum" asp-page-handler="newPosts" class="nowrap">Mesaje noi</a>
    </li>
    <li>
        <span> &nbsp;</span>
        <a asp-page="ViewForum" asp-page-handler="drafts" class="nowrap">Mesaje în lucru</a>
    </li>
    <li>
        <span> &nbsp;</span>
        <a asp-page="/MemberList">Membrii forumului</a>
    </li>

    @foreach (var elem in (Model.ExtraElements ?? Enumerable.Empty<string>()).Where(elem => !string.IsNullOrWhiteSpace(elem)))
    {
        <li>
            <span> &nbsp;</span>
            @Html.Raw(elem)
        </li>
    }
</ul>
